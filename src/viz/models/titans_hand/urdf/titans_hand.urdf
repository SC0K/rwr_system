<?xml version="1.0" ?>
<robot name="converted_robot">

   <!-- World Link -->
   <link name="world"/>

   <!-- Connector Base Link -->
   <link name="connector_base">
      <inertial>
         <origin xyz="0 0 0" rpy="0 0 0"/>
         <mass value="0.07977435345832029"/>
         <inertia ixx="7.214481044209698e-05" iyy="7.16441197786828e-05" izz="1.2264239349426065e-05" ixy="0" ixz="0" iyz="0"/>
      </inertial>
      <visual>
         <origin xyz="0 0 0" rpy="0 0 0"/>
         <geometry>
            <mesh filename="package://viz/models/titans_hand/urdf/converted_mesh/converted_connector_base.stl"/>
         </geometry>
         <material name="white"/>
      </visual>
   </link>

   <!-- Fixed Joint between World and Connector Base -->
   <joint name="world2connector_base" type="fixed">
      <parent link="world"/>
      <child link="connector_base"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
   </joint>

   <!-- Wrist Link -->
   <link name="wrist">
      <inertial>
         <origin xyz="0 0 0" rpy="0 0 0"/>
         <mass value="0.08032373127609675"/>
         <inertia ixx="8.009150976936333e-05" iyy="7.667345488727809e-05" izz="5.544966338921669e-06" ixy="0" ixz="0" iyz="0"/>
      </inertial>
      <visual>
         <origin xyz="0 0 0" rpy="0 0 0"/>
         <geometry>
            <mesh filename="package://viz/models/titans_hand/urdf/converted_mesh/converted_wrist.stl"/>
         </geometry>
         <material name="white"/>
      </visual>
   </link>

   <!-- Revolute Joint between Connector Base and Wrist -->
   <joint type="revolute" name="revolute_base_hand">
      <parent link="connector_base"/>
      <child link="wrist"/>
      <origin xyz="0.0 -0.0388 0.03" rpy="0.0 0.0 0.0"/>
      <axis xyz="0.0 1.0 0.0"/>
      <limit lower="-1.5707" upper="1.5707" effort="100" velocity="100"/>
   </joint>

   <!-- Palm Link -->
   <link name="palm">
      <inertial>
         <origin xyz="0.0004985657687111454 0.012986231765808095 0.0842484060450945" rpy="1.8166143224613616 -0.034173445753463705 6.244104871999667e-05"/>
         <mass value="0.08495943957289792"/>
         <inertia ixx="7.262637133602139e-05" iyy="5.094037047738861e-05" izz="2.3939714595959797e-05" ixy="0" ixz="0" iyz="0"/>
      </inertial>
      <visual>
         <origin xyz="0.0004985657687111454 0.012986231765808095 0.0842484060450945" rpy="1.8166143224613618 -0.034173445753463705 6.244104871999667e-05"/>
         <geometry>
            <mesh filename="package://viz/models/titans_hand/urdf/converted_mesh/converted_palm.stl"/>
         </geometry>
         <material name="white"/>
      </visual>
   </link>

   <!-- Fixed Joint between Wrist and Palm -->
   <joint name="wrist_to_palm" type="fixed">
      <parent link="wrist"/>
      <child link="palm"/>
      <origin xyz="0 0 0.05" rpy="0 0 0"/>
   </joint>

</robot>